<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.lgf.demo">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:name=".App"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.chapterone.RecyclerViewActivity"/>
        <activity android:name=".activity.chapterone.CardViewActivity"/>
        <activity android:name=".activity.chapterone.NotificationActivity"/>
        <activity android:name=".activity.chapterone.PermissionActivity"/>
        <activity android:name=".activity.chapterone.MultiWindowModeActivity" android:configChanges="screenSize|screenLayout"/>
        <activity android:name=".activity.GotoSettingActivity"/>
        <activity android:name=".activity.chaptertwo.LoginActivity"/>
        <activity android:name=".activity.chaptertwo.TabLayoutActivity" />
        <activity android:name=".activity.ChapterTwoActivity"/>
        <activity android:name=".activity.ChapterOneActivity"/>
        <activity android:name=".activity.ChapterThreeActivity"/>
        <activity android:name=".activity.chapterthree.CustomViewActivity"/>
        <activity android:name=".activity.chapterthree.PropertyAnimationActivity"/>
        <activity android:name=".activity.ChapterFiveActivity"/>
        <activity android:name=".activity.chapterfive.VolleyActivity"/>
        <activity android:name=".activity.test.ThreadTestActivity"/>
        <activity android:name=".activity.test.DisplayTestActivity"/>
        <activity android:name=".activity.test.SpannableStringTestActivity"/>
        <activity android:name=".activity.TestActivity"/>
    </application>

</manifest>