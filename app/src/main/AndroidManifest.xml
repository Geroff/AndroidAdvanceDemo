<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.lgf.demo">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RecyclerViewActivity"/>
        <activity android:name=".activity.CardViewActivity"/>
        <activity android:name=".activity.NotificationActivity"/>
        <activity android:name=".activity.PermissionActivity"/>
        <activity android:name=".activity.MultiWindowModeActivity" android:configChanges="screenSize|screenLayout"/>
        <activity android:name=".activity.GotoSettingActivity"/>
        <activity android:name=".activity.LoginActivity"/>
        <activity android:name=".activity.TabLayoutActivity" />
        <activity android:name=".activity.ChapterTwoActivity"/>
        <activity android:name=".activity.ChapterOneActivity"/>
    </application>

</manifest>